#include <iostream>
#include <string>

using namespace std;

class Student
{ 
    private:
    int rollno;
    char lettergrade();
    
    public:
    int getGrade()
    {
        return lettergrade();
    }
    void lettergrade(int numbergrade)
    {
        char letter;
        if (numbergrade > 90)
        {
            letter = 'A';
        }
        else 
        { 
            letter = 'Not A';
        } 
        printf("%s\n",lettergrade());   
    } 
    
    int getRollNo()
    {
        return rollno;
    }
    void setRollno(int i)
    {
        rollno=i;
    }
}A,B;
// A and B are objects of the Student class


int main()
{
    A.setRollno(1);
    B.setRollno(2);
    A.lettergrade(95);
    B.lettergrade(50);
    
}
